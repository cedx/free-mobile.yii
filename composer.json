{
  "description": "Free Mobile connector for the Yii Framework.",
  "homepage": "https://github.com/cedx/yii2-free-mobile",
  "name": "cedx/yii2-free-mobile",

  "license": "Apache-2.0",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "yii2-extension",

  "authors": [{
    "email": "cedric@belin.io",
    "name": "CÃ©dric Belin"
  }],
  "autoload": {
    "psr-4": {"yii\\freemobile\\": "lib/"}
  },
  "config": {
    "optimize-autoloader": true
  },
  "keywords": [
    "client",
    "free",
    "logging",
    "mobile",
    "sms",
    "yii2"
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "psr/http-message": "^1.0.1",
    "yiisoft/yii2": "^2.0.11",
    "yiisoft/yii2-httpclient": "^2.0.3"
  },
  "require-dev": {
    "cedx/coveralls": "^0.4.0",
    "cedx/phpunit-expect": "^0.1.0",
    "phing/phing": "^2.16.0",
    "phpdocumentor/phpdocumentor": "^2.9.0"
  },
  "scripts": {
    "coverage": "coveralls --file=var/coverage.xml",
    "test": "phpunit --colors=auto --verbose"
  },
  "support": {
    "issues": "https://github.com/cedx/yii2-free-mobile/issues"
  }
}
