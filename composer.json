{
  "description": "Send SMS messages to your Free Mobile device. A simple and free way to monitor your applications using a Free Mobile account.",
  "homepage": "https://dev.belin.io/yii2-free-mobile",
  "license": "MIT",
  "name": "cedx/yii2-free-mobile",
  "type": "yii2-extension",
  "authors": [
    {"email": "cedric@belin.io", "name": "CÃ©dric Belin", "homepage": "https://belin.io"}
  ],
  "autoload": {
    "psr-4": {"yii\\freemobile\\": "lib/"}
  },
  "autoload-dev": {
    "files": ["vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"],
    "psr-4": {"yii\\freemobile\\": "test/"}
  },
  "config": {
    "fxp-asset": {"enabled": false},
    "optimize-autoloader": true
  },
  "keywords": [
    "client",
    "free",
    "logging",
    "mobile",
    "sms",
    "yii2"
  ],
  "repositories": [
    {"type": "composer", "url": "https://asset-packagist.org"}
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/psr7": "^1.6.1",
    "yiisoft/yii2": "^2.0.22",
    "yiisoft/yii2-httpclient": "^2.0.11"
  },
  "require-dev": {
    "cedx/coveralls": "^10.1.0",
    "consolidation/robo": "^1.4.9",
    "henrikbjorn/lurker": "^1.2.0",
    "phpstan/phpstan": "^0.11.12",
    "phpunit/phpunit": "^8.2.4"
  },
  "scripts": {
    "coverage": "robo coverage",
    "test": "robo test"
  },
  "support": {
    "docs": "https://dev.belin.io/yii2-free-mobile/api",
    "issues": "https://github.com/cedx/yii2-free-mobile/issues"
  }
}
