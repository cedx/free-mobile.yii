{
  "description": "Free Mobile connector for the Yii Framework.",
  "homepage": "https://github.com/cedx/yii2-free-mobile",
  "name": "cedx/yii2-free-mobile",

  "license": "Apache-2.0",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "library",

  "authors": [{
    "email": "cedric@belin.io",
    "name": "CÃ©dric Belin"
  }],
  "autoload": {
    "psr-4": {"yii\\freemobile\\": "lib/"}
  },
  "config": {
    "optimize-autoloader": true
  },
  "keywords": [
    "client",
    "free",
    "logging",
    "mobile",
    "sms",
    "yii2"
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-curl": "*",
    "cedx/free-mobile": "^3.1.0",
    "psr/http-message": "^1.0.1",
    "yiisoft/yii2": "^2.0.11"
  },
  "require-dev": {
    "phing/phing": "^2.16.0",
    "phpdocumentor/phpdocumentor": "^2.9.0",
    "phpmd/phpmd": "^2.6.0",
    "phpunit/phpunit": "^5.7.12",
    "satooshi/php-coveralls": "^1.0.1"
  },
  "scripts": {
    "coverage": "coveralls --coverage_clover=var/coverage.xml",
    "test": "phpunit"
  },
  "support": {
    "issues": "https://github.com/cedx/yii2-free-mobile/issues"
  }
}
